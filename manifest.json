{
    "manifest_version": 2,
    "name": "YTPresence",
    "version": "1.0",
    "permissions": [
      "tabs",
      "https://www.youtube.com/*"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*"],
        "js": ["content_script.js"]
      }
    ]
  }
  
